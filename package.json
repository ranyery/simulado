{
  "name": "@simulado/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start:admin": "npx nx serve admin",
    "start:client": "npx nx serve client",
    "start:server": "npx nx serve server",
    "build:admin": "npx nx build admin",
    "build:client": "npx nx build client",
    "build:server": "npx prisma generate && npx prisma migrate deploy && npx nx build server",
    "build:all": "npm run build:admin && npm run build:client && npm run build:server",
    "postbuild:admin": "npm run purge-css:admin",
    "postbuild:client": "npm run purge-css:client",
    "lint": "npm run lint:styles && npm run lint:all-projects",
    "lint:styles": "npx stylelint ./**/*.scss --fix",
    "lint:all-projects": "npx nx lint admin && npx nx lint client && npx nx lint server",
    "format": "npx prettier --log-level warn ./**/*.{html,scss,json,js,ts} --write && npx nx format:write --all",
    "purge-css:admin": "npx purgecss -css dist/apps/admin/*.css --content dist/apps/admin/index.html dist/apps/admin/*.js --safelist p-toast-message-info p-toast-message-success p-toast-message-warn p-toast-message-error -o dist/apps/admin/",
    "purge-css:client": "npx purgecss -css dist/apps/client/*.css --content dist/apps/client/index.html dist/apps/client/*.js -o dist/apps/client/",
    "install:clean": "rm -rf node_modules/ && npm cache clean --force && npm ci",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.2.9",
    "@angular/cdk": "^16.2.8",
    "@angular/common": "~16.2.9",
    "@angular/compiler": "~16.2.9",
    "@angular/core": "~16.2.9",
    "@angular/forms": "~16.2.9",
    "@angular/platform-browser": "~16.2.9",
    "@angular/platform-browser-dynamic": "~16.2.9",
    "@angular/router": "~16.2.9",
    "@kolkov/angular-editor": "3.0.0-beta.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^10.2.7",
    "@nrwl/angular": "16.10.0",
    "@prisma/client": "^5.4.2",
    "@vercel/analytics": "^1.1.1",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "extract-math": "^1.2.3",
    "jwt-decode": "^3.1.2",
    "katex": "^0.16.9",
    "nestjs-zod": "^3.0.0",
    "ngx-pipes": "^3.2.2",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^16.3.1",
    "reflect-metadata": "^0.1.13",
